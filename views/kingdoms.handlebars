<h1>{{title}}</h1>

<form action="/kingdoms" method="post">
	<label for="king">King</label>
	<input type="text" name="king">

	<label for="queen">Queen</label>
	<input type="text" name="queen">

	<label for="kingdom">Kingdom</label>
	<input type="text" name="kingdom">

	<button>Submit</button>
</form>

<ul>
{{#each data as |item|}}
	<a href="{{../currentPath}}/{{item}}"><li>{{item}}</li></a>
{{/each}}
</ul>