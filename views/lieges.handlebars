<div class='main'>
  <h1 class='highlighted'>Lieges of {{castle.name}} castle</h1>
  <div class="container">
    {{#each castle.lieges as |liege|}}
    <div class="kingdom focus" id={{liege.id}}>
      <div class='image'>
        <a href='#'>
          <h2 class="grayed">{{liege.name}}</h2>
        </a>
      </div>
      <section>
        <h4 class="grayed"># of Vassals: {{liege.vassalIds.length}}</h4>
      </section>
    </div>
    {{/each}}
  </div>
